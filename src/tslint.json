{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "indent": [true, "spaces", 4],
        "semicolon": false,
        "object-literal-shorthand": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false
    },
    "rulesDirectory": []
}
